import forge;
import forge.log as log;
import forge.result as result;

fun safe_div(a:: i32, b:: i32) -> result<i32, str> {
    if b == 0 {
        return result.err("Division by zero");
    }
    return result.ok(a / b);
}

fun apex() {
    log.info("=== Error Handling Examples ===");
    log.info("10 / 2 = ", safe_div(10, 2));
    log.info("10 / 0 = ", safe_div(10, 0));
}
