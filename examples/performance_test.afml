import forge;
import forge.log as log;
import forge.vec as vec;

fun fib(n:: i32) -> i32 {
    if n <= 1 {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}

fun apex() {
    log.info("=== AFNS Performance Tests ===");
    log.info("Test 1: Fibonacci(10)");
    log.info("Result: ", fib(10));

    log.info("Test 2: Vector Operations");
    let values = vec.new();
    var i = 0;
    while i < 5 {
        vec.push(values, i * 2);
        i = i + 1;
    }
    log.info("Vector length: ", vec.len(values));
}
